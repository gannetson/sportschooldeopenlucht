	  <sf:bulkRetrieve username="${sf.dev.username}" password="${sf.dev.password}" serverurl="${sf.test.serverurl}" retrieveTarget="retrieve_var_dev/EmailTemplate" metadataType="EmailTemplate" containingFolder="unfiled$public"/>
