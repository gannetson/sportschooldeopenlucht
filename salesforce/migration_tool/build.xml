		<copy todir="prepare_var_dev"><fileset dir="retrieve_var_dev"/></copy>
		<copy todir="prepare_obj_dev"><fileset dir="retrieve_obj_dev"/></copy>
		<copy todir="prepare_rep_dev"><fileset dir="retrieve_rep_dev"/></copy>
		<copy todir="prepare_apx_dev"><fileset dir="retrieve_apx_dev"/></copy>
