    <property environment="env"/>
	
	<!-- Retrieve all metadata from all environments -->
	<target name="retrieve_all">
	  <antcall target="retrieve_var_dev"/>
	  <antcall target="retrieve_obj_dev"/>
	  <antcall target="retrieve_rep_dev"/>
	  <antcall target="retrieve_apx_dev"/>
	  <antcall target="retrieve_var_uat"/>
	  <antcall target="retrieve_obj_uat"/>
	  <antcall target="retrieve_rep_uat"/>
	  <antcall target="retrieve_apx_uat"/>
	  <antcall target="retrieve_var_prd"/>
	  <antcall target="retrieve_obj_prd"/>
	  <antcall target="retrieve_rep_prd"/>
	  <antcall target="retrieve_apx_prd"/>
	</target>
	
	<!-- Remove data from all retrieve directories -->
	<target name="empty_retrieve">
		<delete includeemptydirs="true">
			<fileset dir="retrieve_var_dev" includes="**/*"/>
			<fileset dir="retrieve_obj_dev" includes="**/*"/>
			<fileset dir="retrieve_rep_dev" includes="**/*"/>
			<fileset dir="retrieve_apx_dev" includes="**/*"/>
			<fileset dir="retrieve_var_uat" includes="**/*"/>
			<fileset dir="retrieve_obj_uat" includes="**/*"/>
			<fileset dir="retrieve_rep_uat" includes="**/*"/>
			<fileset dir="retrieve_apx_uat" includes="**/*"/>
			<fileset dir="retrieve_var_prd" includes="**/*"/>
			<fileset dir="retrieve_obj_prd" includes="**/*"/>
			<fileset dir="retrieve_rep_prd" includes="**/*"/>
			<fileset dir="retrieve_apx_prd" includes="**/*"/>
		</delete>
	</target>
	
	<!-- Remove data from all prepare directories -->
	<target name="empty_prepare">
		<delete includeemptydirs="true">
			<fileset dir="prepare_var_dev" includes="**/*"/>
			<fileset dir="prepare_obj_dev" includes="**/*"/>
			<fileset dir="prepare_rep_dev" includes="**/*"/>
			<fileset dir="prepare_apx_dev" includes="**/*"/>
			<fileset dir="prepare_var_uat" includes="**/*"/>
			<fileset dir="prepare_obj_uat" includes="**/*"/>
			<fileset dir="prepare_rep_uat" includes="**/*"/>
			<fileset dir="prepare_apx_uat" includes="**/*"/>
